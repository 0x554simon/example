<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
   <data-sources />
   <form-beans >
      <form-bean name="EtnetForwardForm" type="com.itsz.web.rtq.actionform.EtnetForwardForm" />
     
   </form-beans>

   <global-exceptions />
  
   <global-forwards >
      <forward name="errorRTQ" path="/errorRTQ.jsp" />      
   </global-forwards>
   
   <action-mappings >  
    <action
      path="/SelectQuotation"
      type="com.itsz.web.rtq.action.SelectQuotationAction"
      validate="false" >
      <forward name="qs2" path="/rtq_qs2.jsp" redirect="false"/>
      <forward name="powerticker" path="/rtq_powerticker.jsp" redirect="false"/>
      <forward name="etnet" path="/rtq_etnet.jsp" redirect="false"/>
    </action>
    
    
    
    <action path="/BringPrice"
        type="org.apache.struts.actions.ForwardAction"
        parameter="/bringprice.jsp"/>
        
    <action
      name="EtnetForwardForm"
      path="/EtnetForward"
      scope="request"
      type="com.itsz.web.rtq.action.EtnetForwardAction"
      validate="false">
      <forward name="etnet_failure" path="/etnet_failure.jsp" redirect="false"/>
      <forward name="etnet_popup" path="/etnet_server.jsp" redirect="false"/>
      <forward name="etnet_new_popup" path="/etnet_new_server.jsp" redirect="false"/>
      
    </action> 
    <action
     path="/changLang"
     scope="request"
     type="com.itsz.web.rtq.action.ChangLangAction">
     	<forward name="success" path="/main.jsp"></forward>
     	<forward name="failed" path="/index.do"></forward>
     </action>
    <action
     path="/login"
     scope="request"
     type="com.itsz.web.rtq.action.LoginAction">
     	<forward name="success" path="/main.jsp"></forward>
     </action>
    <action
     path="/index"
     scope="request"
     type="com.itsz.web.rtq.action.InitLoginAction">
     	<forward name="success" path="/login.jsp"></forward>
     </action>
    <action
     path="/logout"
     scope="request"
     type="com.itsz.web.rtq.action.LogoutAction">
     	<forward name="success" path="/index.do"></forward>
     	<forward name="failed" path="/index.do"></forward>
     </action>
</action-mappings>

	<message-resources parameter="com.itsz.Contingency.resource.ContingencyResource" />
	
</struts-config>

