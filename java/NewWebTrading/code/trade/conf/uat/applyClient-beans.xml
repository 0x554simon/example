<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
    http://www.springframework.org/schema/beans/spring-beans-2.0.xsd   
    http://cxf.apache.org/jaxws http://cxf.apache.org/schema/jaxws.xsd">

	<bean id="eipoApplyWebService" class="com.itsz.eipo.webservice.ApplyWebService"
		factory-bean="clientFactory" factory-method="create" />

	<bean id="clientFactory"
		class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">
		<property name="serviceClass" value="com.itsz.eipo.webservice.ApplyWebService" />
		<!-- <property name="address" value="http://10.100.97.56:9080/eipo/services/eipo_apply" /> -->
		<property name="address" value="http://10.100.97.56:9080/eipo/services/eipo_apply" />
		<!--10.100.97.56:9080 -->
	</bean>

</beans>