			attribute="ProfitAndLossEnquiryForm"
			name="ProfitAndLossEnquiryForm"
			path="/ProfitAndLossEnquiryWithSP"
			scope="request"
			type="com.itsz.web.mcs.action.ProfitAndLossEnquiryWithSPAction"
			validate="false" />	
		
		<action
			path="/BankList"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/web/error/blank_cps.jsp"/>   
		
		<action
			path="/RTQForward"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/quot_forward.jsp"/>            
		
		<action
		    name="itszForm"
			path="/RTQForwardInit"
			type="com.itsz.sht.struts.action.AccessRTQAction"
			validate="false">
				<forward name="no_purchased" path="/jsp/no_purchased.jsp" contextRelative="true"/>
				<forward name="es_exception" path="/jsp/es_exception2.jsp" contextRelative="true"/>
				<forward name="aastock" path="/jsp/RTQ_AAS-Submit.jsp" contextRelative="true"/>
				<forward name="etnet" path="/jsp/rtq_etnet_ps.jsp" contextRelative="true"/>
				<forward name="stt_fail" path="/jsp/session_overtime.jsp" contextRelative="true"/>
		</action>
		
		<action
			name="EtnetForwardForm"
			path="/EtnetForward"
			scope="request"
			type="com.itsz.sht.struts.action.EtnetForwardAction"
			validate="false">
				<forward name="etnet_failure" path="/jsp/etnet_failure.jsp" contextRelative="true" redirect="false"/>
				<forward name="etnet_post" path="/jsp/RTQ_ETNET-Submit.jsp" contextRelative="true" redirect="false"/>
		</action> 
                
		<!--============================================================-->
		<action
	      path="/ePaymentTransfer"
	      type="com.itsz.sht.struts.action.EpaymentTransferAction"
	      name="itszForm"
	      scope="request"
	    >
	      <forward name="success" path="/jsp/transfer/epayment_forward.jsp" contextRelative="true"/>
	      <forward name="failure" path="/jsp/blank.jsp" contextRelative="true"/>
	    </action>
	    
		<action
			path="/ppsTransferForward"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/transfer/pps_top.jsp"/>
		
		<action
			path="/ppsHandleFeedback"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/transfer/DecDR.jsp"/>

		<action
			path="/ppsTransferInit"
			type="com.itsz.sht.struts.action.PPSTransferInitAction"
			name="ppsTransferForm"
			scope="request"
			input="/jsp/transfer/eps_transfer1.jsp"
			unknown="false"
			validate="false">
				<forward name="ppsForward" path="/jsp/transfer/eps_transfer1.jsp" contextRelative="true"/>     
				<forward name="failure" path="/jsp/transfer/eps_transfer1.jsp" contextRelative="true"/>
				<forward name="suspend" path="/jsp/transfer/pps_suspend.jsp" contextRelative="true"/>
				<forward name="nonoperate" path="/jsp/transfer/pps_nonoperate.jsp" contextRelative="true"/>
				<forward name="inactive" path="/jsp/transfer/pps_inactive.jsp" contextRelative="true"/>
				<forward name="epayment_session_overtime" path="/jsp/session_overtime.jsp" contextRelative="true"/>
      </action>
      
      <action
		  path="/ppsTransfer"
		  type="com.itsz.sht.struts.action.PPSTransferPSAction"
		  name="ppsTransferForm"
		  scope="request"
		  unknown="false"
		  validate="true">
			  <forward name="success" path="/jsp/transfer/pps_forward.jsp" redirect="false" contextRelative="true"/>
			  <forward name="failure" path="/ppsTransferInit.do" redirect="false"/>
			  <forward name="suspend" path="/jsp/transfer/pps_suspend.jsp" redirect="false" contextRelative="true"/>
			  <forward name="nonoperate" path="/jsp/transfer/pps_nonoperate.jsp" redirect="false" contextRelative="true"/>
			  <forward name="inactive" path="/jsp/transfer/pps_inactive.jsp" redirect="false" contextRelative="true"/>
			  <forward name="epayment_session_overtime" path="/jsp/session_overtime.jsp" contextRelative="true"/>
		</action>

		<action
			path="/ppsEnquiry"
			type="com.itsz.sht.struts.action.PPSEnquiryPSAction"
			name="ppsEnquiryForm"
			scope="request"
			input="/jsp/transfer/mkenq.jsp"
			unknown="false"
			validate="false">
				<forward name="ppsEnquiryForward" path="/jsp/transfer/mkenq.jsp" redirect="false" contextRelative="true"/>
				<forward name="failure" path="/ppsTransferInit.do" redirect="false"/>
				<forward name="epayment_session_overtime" path="/jsp/session_overtime.jsp" contextRelative="true"/>
		</action>
    
		<action path="/bocTransferInit" 
    		type="com.itsz.sht.struts.action.BOCTransferInitAction"
			name="bocTransferForm"
			scope="request"
			input="/jsp/transfer/boc_transfer.jsp"
			validate="false"
			unknown="false">
				<forward name="bocForward" path="/jsp/transfer/boc_transfer.jsp" redirect="false" contextRelative="true"></forward>
				<forward name="failure" path="/jsp/transfer/boc_transfer.jsp" redirect="false" contextRelative="true"></forward>
				<forward name="suspend" path="/jsp/transfer/boc_suspend.jsp" redirect="false" contextRelative="true"></forward>
				<forward name="nonoperate" path="/jsp/transfer/boc_nonoperate.jsp" redirect="false" contextRelative="true"/>
				<forward name="inactive" path="/jsp/transfer/boc_inactive.jsp" redirect="false" contextRelative="true"></forward>
				<forward name="epayment_session_overtime" path="/jsp/session_overtime.jsp" contextRelative="true"/>
		</action>	
    
		<action path="/bocTransfer"
    		type="com.itsz.sht.struts.action.BOCTransferPSAction"
    		name="bocTransferForm"
    		scope="request"
    		validate="true"
    		unknown="false">
				<forward name="success" path="/jsp/transfer/boc_forward.jsp" redirect="false" contextRelative="true"/>
			   <forward name="failure" path="/bocTransferInit.do" redirect="false"/>
			   <forward name="suspend" path="/jsp/transfer/boc_suspend.jsp" redirect="false" contextRelative="true"/>
			   <forward name="nonoperate" path="/jsp/transfer/boc_nonoperate.jsp" redirect="false" contextRelative="true"/>
			   <forward name="inactive" path="/jsp/transfer/boc_inactive.jsp" redirect="false" contextRelative="true"></forward>
			   <forward name="epayment_session_overtime" path="/jsp/session_overtime.jsp" contextRelative="true"/>
		</action>
       
		<action
			path="/bocTransferForward"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/transfer/boc_top.jsp"/>
	
		<action
			path="/sapforward"
			scope="request"
			validate="false"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/sp/spForward.jsp"/>
		
		<action
			name="itszForm"
			path="/spReport"
			scope="request"
			validate="false"
			type="com.itsz.channelsserver.struts.action.SapReportAction">
				<forward name="report" contextRelative="true" path="/jsp/sp/spReport.jsp"/>
				<forward name="failure" contextRelative="true" path="/jsp/session_overtime.jsp"/>
		</action>
		
		<action
			path="/shkforward"
			scope="request"
			validate="false"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/shk/shkForward.jsp"/>
		
		<action
			name="itszForm"
			path="/shkSubmit"
			scope="request"
			validate="false"
			type="com.itsz.sht.struts.action.ShkSubmitAction">
				<forward name="success" contextRelative="true" path="/jsp/shk/shkSubmit.jsp"/>
				<forward name="failure" contextRelative="true" path="/jsp/session_overtime.jsp"/>
		</action>
		<action
			name="itszForm"
			path="/ahSubmit"
			scope="request"
			validate="false"
			type="com.itsz.sht.struts.action.AHSubmitAction">
				<forward name="success" contextRelative="true" path="/jsp/ahquote/ahSubmit.jsp"/>
				<forward name="failure" contextRelative="true" path="/jsp/ahquote/ahError.jsp"/>
				<forward name="epayment_session_overtime" path="/jsp/session_overtime.jsp" contextRelative="true"/>
		</action>
		<action
			name="queryConfigForm"
			path="/queryConfig"
			scope="request"
			type="com.itsz.channelsserver.struts.action.QueryConfigAction"
			validate="false" />
		<action
			name="tradeListForm"
			path="/tradeList"
			scope="request"
			type="com.itsz.sht.struts.action.TradeListAction"
			validate="false" />			
		<action
			path="/exportforward"
			scope="request"
			validate="false"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/transfer/exportForward.jsp"/>
		<action
			name="tradeListForm"
			path="/exportTradeList"
			scope="request"
			type="com.itsz.sht.struts.action.ExportTradeListAction"
			validate="false">
				<forward name="success" contextRelative="true" path="/jsp/transfer/exportTLExecl.jsp"/>
		</action>
		<action
			name="enquireRTQForm"
			path="/enquireRTQ"
			scope="request"
			type="com.itsz.channelsserver.struts.action.EnquireRTQAction"
			validate="false" />
		<action
			name="snapshotProfileForm"
			path="/SnapshotProfile"
			scope="request"
			type="com.itsz.channelsserver.struts.action.SnapshotProfileAction"
			validate="false" />			
		<action
			name="streamRtqProfileForm"
			path="/StreamRtqProfile"
			scope="request"
			type="com.itsz.channelsserver.struts.action.AccessRTQgetURLAction"
			validate="false" />
		
