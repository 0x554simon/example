			name="enquirePositionForm"
			path="/StockHoldings2"
			scope="request"
			type="com.itsz.channelsserver.struts.action.StockHoldings2Action"
			validate="false" />

		<action
			path="/ListNews"
			type="org.apache.struts.actions.ForwardAction"
			parameter="/jsp/sp/sp_forward.jsp"/>
      
