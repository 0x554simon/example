<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.itsz.sht.dao.hibernate.model.FundDeposit"
		table="NW_FUND_DEPOSIT">	
         <id name="fundDepositId" column="FUND_DEPOSIT_ID" type="java.lang.Long">
            <generator class="sequence">
               <param name="sequence">NW_SEQ_FUND_DEPOSIT</param>
            </generator>
        </id>
        <property name="receiveDate">
			<column name="Receive_Date" sql-type="timestamp"/>
		</property>
		<property name="depositDate">
			<column name="Deposit_Date" sql-type="timestamp"/>
		</property>
        <property name="accountName" column="ACCOUNT_NAME" type="java.lang.String" />
        <property name="accountNo" column="ACCOUNT_NO" type="java.lang.String" />
        <property name="currency" column="CURRENCY" type="java.lang.String" />
        <property name="amount" column="AMOUNT" type="java.math.BigDecimal" />        
        <property name="bank" column="BANK" type="java.lang.String" />
        <property name="bankAcc" column="BANK_ACC" type="java.lang.String" />
        <property name="source" column="SOURCE" type="java.lang.String" />
        <property name="Reference" column="REFERENCE" type="java.lang.String" />
        <property name="Remarks" column="REMARKS" type="java.lang.String" />
        <property name="isSended" column="ISSENDED" type="java.lang.String" />
        <property name="requestNo" column="REQUESTNO" type="java.lang.String" />
	</class>
</hibernate-mapping>