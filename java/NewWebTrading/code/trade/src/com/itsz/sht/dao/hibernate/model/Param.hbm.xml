<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.itsz.sht.dao.hibernate.model.Param"
		table="SERVICE_PARAM">	
		<composite-id name="id"
			class="com.itsz.sht.dao.hibernate.model.ParamPK">
			<key-property name="paramName" column="param"/>
			<key-property name="serviceID" column="service_id"/>
		</composite-id>
		<property name="paramValue">
			<column name="val" sql-type="varchar2(30)"/>
		</property>
		<property name="dataType">
			<column name="data_type" sql-type="varchar2(30)"/>
		</property>
		<property name="updateSeqNum">
			<column name="upd_seq_num" sql-type="number(18)"/>
		</property>
		<property name="lastModifiedDate">
			<column name="last_modified" sql-type="timestamp(6)"/>
		</property>
		<property name="updateToken">
			<column name="upd_token" sql-type="varchar2(50)"/>
		</property>
		
	</class>
</hibernate-mapping>