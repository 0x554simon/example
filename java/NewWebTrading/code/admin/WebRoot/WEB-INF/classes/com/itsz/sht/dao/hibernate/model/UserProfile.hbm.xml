<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
       <class name="com.itsz.sht.dao.hibernate.model.UserProfile" table="NW_USR_PROF">
        <id name="clntId" type="java.lang.String">
            <column name="CLNT_ID" length="20" />
            <generator class="assigned" />
        </id>
        <property name="clntLoginId" type="java.lang.String">
            <column name="CLNT_LOGIN_ID" length="20" />
        </property>
        <property name="defDebtAcc" type="java.lang.String">
            <column name="DEF_DEBT_ACC" length="20" />
        </property>
        <property name="cnDiscFlag" type="java.lang.String">
            <column name="CN_DISC_FLAG" length="20" />
        </property>
        <property name="initBy" type="java.lang.String">
            <column name="INIT_BY" length="20" />
        </property>
        <property name="initDate" type="java.util.Date">
            <column name="INIT_DATE" length="7" />
        </property>
        <property name="updBy" type="java.lang.String">
            <column name="UPD_BY" length="20" />
        </property>
        <property name="updDate" type="java.util.Date">
            <column name="UPD_DATE" length="7" />
        </property>
    </class>
</hibernate-mapping>